
@article{Al-Tamimi2017,
  title = {Effect of {{Geometry}} of {{Holes}} on {{Heat Transfer}} of {{Concrete Masonry Bricks Using Numerical Analysis}}},
  author = {{Al-Tamimi}, Ahmed S. and {Al-Osta}, Mohammed A. and {Al-Amoudi}, Omar S. Baghabra and {Ben-Mansour}, Rached},
  year = {2017},
  month = sep,
  journal = {Arabian Journal for Science and Engineering},
  volume = {42},
  number = {9},
  pages = {3733--3749},
  issn = {2191-4281},
  doi = {10.1007/s13369-017-2482-6},
  abstract = {The population growth and harsh weather conditions in Saudi Arabia increase the demand for electricity due to the overwhelming usage of air-conditioning system for cooling during the excessively hot weather summer conditions. Therefore, the demand to produce efficient insulation materials for building construction has recently increased for energy and environment conservation, particularly with the recent increase in the electricity tariffs. In this paper, finite element analysis using ABAQUS software was used to determine the optimum geometry of holes for concrete bricks that would increase the thermal resistance of the wall, thereby reducing the consumption of electrical power. The analysis was carried out for 23 models with different configurations of holes for masonry concrete bricks. Regular and staggered cavities arrangements were analyzed. The conduction, convection and thermal radiation phenomena were considered for solid and hollow masonry bricks. The results demonstrated that 51\% hollow ratio could reduce the average inner surface temperature \$\$(T\_\textbackslash mathrm\{i\})\$\$of the concrete brick by \$\$7.18\textbackslash,\{\^\{\textbackslash circ \}\}\textbackslash hbox \{C\}\$\$. For bricks with the same hollow ratio, it was observed that there is an effect of the shape of holes in reducing the thermal flow through the bricks, while the number of holes has a marginal effect on the average inner surface temperature \$\$(T\_\textbackslash mathrm\{i\})\$\$. Further, the staggered arrangement for circular holes has considerable effect in reducing \$\$T\_\textbackslash mathrm\{i\}\$\$for bricks with four cavities, while the rectangular cavities have negative impact on \$\$T\_\textbackslash mathrm\{i\}\$\$. Moreover, for the models with circular and rectangular eight cavities, the staggered arrangement has an advantage in reducing \$\$(T\_\textbackslash mathrm\{i\})\$\$.},
  langid = {english},
  keywords = {Conduction; Convection,Finite element model,Hollow concrete bricks,IMPORTANT,Radiation,READED,Thermal}
}

