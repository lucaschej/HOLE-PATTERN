  author = {Wong, M. Y. and Tso, C. Y. and Ho, T. C. and Lee, H. H.},
  year = {2021},
  month = jan,
  journal = {International Journal of Heat and Mass Transfer},
  volume = {164},
  pages = {120607},
  issn = {0017-9310},
  doi = {10.1016/j.ijheatmasstransfer.2020.120607},
  abstract = {Thermal management is an essential aspect of various applications, especially in the area of electronic miniaturization. Thermal diodes, which exhibit asymmetric heat transfer characteristics along an axial direction, are one of the vital components for use in thermal management systems. This review summarizes both theoretical and experimental studies of various types of thermal diodes introduced in recent years. The thermal diodes discussed in this review are classified based on their primary heat transfer mechanisms and the materials from which they are constructed. After gaining an understanding of the thermal rectification mechanisms of these thermal diodes, we also discuss several applications of thermal diodes, including solar power systems, caloric refrigeration systems, and thermal computing. Lastly, we present the application potential of the thermal diodes in several thermal systems that have not been investigated before, such as temperature controllers, micro-electronic cooling, space technologies, atomic clocks, and bio-technologies.},
  langid = {english},
  keywords = {Heat Transfer,READED,Thermal Conductivity,Thermal Diodes,Thermal Management,Thermal Rectification},
  file = {C\:\\Users\\lucas\\Zotero\\storage\\WS3FQ5HI\\S0017931020335432.html}
}

